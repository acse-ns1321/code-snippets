// VECTOR
for (double i = 0; i < 10; i++)
	{
		v1.push_back(i);
	}
	

// SET
for (double i = 0; i < 10; i++)
{
    s1.insert(i);
}

// MAP
for (double i = 0; i < 10; i++){
    m1.insert(make_pair(i, i * 2));
}



// VECTOR OF TUPLES
 for (int i = 0; i < 10; i++){
        tuples.push_back(std::make_tuple(i, i+1, i+2));
    }
   
for (auto [ X, Y, Z ] : tuples)    {
      std::cout << X << " " << Y << " " << Z << std::endl;
    }
